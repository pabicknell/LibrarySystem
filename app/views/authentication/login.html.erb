<h1>Paul's Library Management System</h1>

<h2>Log In</h2>
<%= form_tag authentication_login_path do |form| %>
  <div class="field">
    <%= label_tag :email %>
    <%= text_field_tag :email %><br />
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %><br />
  </div>
  <div class="actions">
    <%= submit_tag "Login" %>
  </div>
<% end %>

